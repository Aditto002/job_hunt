{
  "expo": {
    "name": "Job_hunt",
    "slug": "Job_hunt",
    "scheme": "jobhunt",
    "platforms": ["ios", "android"],
    "ios": {
      "bundleIdentifier": "com.example.jobhunt",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.example.jobhunt",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "www.jobhunt.com",  
            "pathPrefix": "/success-payment"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ] 

    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
   
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
